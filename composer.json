{
  "name": "surfnet/stepup-webauthn",
  "license": "Apache-2.0",
  "description": "WebAuthn Generic SAML Stepup Provider.",
  "type": "project",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Surfnet\\Webauthn\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dev\\": "dev",
      "Test\\": "tests"
    }
  },
  "require": {
    "php": "8.2.*",
    "ext-dom": "*",
    "ext-gmp": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-zlib": "*",
    "composer/package-versions-deprecated": "^1.11.99",
    "doctrine/annotations": "^2.0",
    "doctrine/common": "^3.4",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^2.17",
    "incenteev/composer-parameter-handler": "^v2.1",
    "nyholm/psr7": "^1.8",
    "openconext/monitor-bundle": "^4.0",
    "ramsey/uuid": "^4.7",
    "spomky-labs/cbor-bundle": "*",
    "spomky-labs/cbor-php": "^3.0",
    "surfnet/stepup-bundle": "^6.0",
    "surfnet/stepup-gssp-bundle": "^5.0",
    "surfnet/stepup-saml-bundle": "^6.0",
    "symfony/asset": "^6.4",
    "symfony/cache": "6.*.*",
    "symfony/config": "^6.4",
    "symfony/console": "^6.4",
    "symfony/flex": "^2.4",
    "symfony/form": "^6.4",
    "symfony/http-client": "^6.4",
    "symfony/monolog-bundle": "^3.8",
    "symfony/runtime": "6.*.*",
    "symfony/security-bundle": "^6.4",
    "symfony/stimulus-bundle": "^2.15",
    "symfony/templating": "^6.4",
    "symfony/translation": "^6.4",
    "symfony/twig-bundle": "^6.4",
    "symfony/validator": "^6.4",
    "symfony/webpack-encore-bundle": "^2.1",
    "symfony/yaml": "^6.4",
    "twig/twig": "^3.8",
    "web-auth/cose-lib": "^4.2",
    "web-auth/webauthn-lib": "^4.7",
    "web-auth/webauthn-stimulus": "*",
    "web-auth/webauthn-symfony-bundle": "^4.7",
    "web-token/jwt-signature-algorithm-rsa": "^3.2"
  },
  "require-dev": {
    "malukenho/docheader": "^1",
    "mockery/mockery": "^1.6",
    "overtrue/phplint": "*",
    "phpmd/phpmd": "^2.14",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-symfony": "^1.3",
    "phpunit/phpunit": "^9.6",
    "sebastian/phpcpd": "^6.0",
    "slevomat/coding-standard": "^8.13",
    "spatie/phpunit-snapshot-assertions": "^4.2",
    "squizlabs/php_codesniffer": "^3.7",
    "symfony/browser-kit": "^6.4",
    "symfony/css-selector": "^6.4",
    "symfony/phpunit-bridge": "^6.4",
    "symfony/stopwatch": "^6.4",
    "symfony/web-profiler-bundle": "^6.4"
  },
  "scripts": {
    "check": [
      "@composer-validate",
      "@docheader",
      "@lint",
      "@phpcs",
      "@phpcpd",
      "@phpmd",
      "@phpstan",
      "@unit-tests",
      "@jest"
    ],
    "composer-validate": "./ci/qa/validate",
    "docheader": "./ci/qa/docheader",
    "lint": "./ci/qa/lint",
    "phpcs": "./ci/qa/phpcs",
    "phpcpd": "./ci/qa/phpcpd",
    "phpcbf": "./ci/qa/phpcbf",
    "phpmd": "./ci/qa/phpmd",
    "phpstan": "./ci/qa/phpstan",
    "phpstan-baseline": "./ci/qa/phpstan-update-baseline",
    "unit-tests": "ci/qa/phpunit",
    "jest": "yarn jest",
    "tsc": "yarn tsc --noEmit",
    "frontend-install": [
      "yarn install"
    ],
    "encore": [
      "./node_modules/.bin/encore"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install": "symfony-cmd"
    },
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "@auto-scripts"
    ]
  },
  "archive": {
    "exclude": [
      "!/.env",
      "!/.env.local.php",
      "!/var/cache/prod",
      "!/public/build",
      ".github",
      ".gitignore",
      "/symfony.lock",
      "/node_modules",
      "/assets",
      "/ci",
      "/dev",
      "/translations/sp",
      "tests",
      "/build",
      "/tsconfig.json",
      "/tslint.json",
      "/Vagrantfile",
      "/webpack.config.js",
      "/jest.config.js",
      "/.docheader",
      "/.env.ci",
      "/.env.test",
      "/.env.vm",
      "/*.xml",
      "/*.xml.dist",
      "/config/routes/dev",
      "/config/packages/dev",
      "/config/packages/test",
      "/node_modules",
      "/build",
      "/docker"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "runtime": {
      "disable_dotenv": true
    },
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*"
    },
    "incenteev-parameters": [
      {
        "file": "config/openconext/parameters.yaml"
      }
    ]
  }
}
